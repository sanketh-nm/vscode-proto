{
  "name": "vscode-proto",
  "displayName": "vscode-proto",
  "description": "proto3 support for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:proto3",
    "onCommand:proto3.compile.one",
    "onCommand:proto3.compile.all"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-proto.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "proto3",
        "extensions": [
          ".proto"
        ],
        "aliases": [
          "Protocol Buffers"
        ]
      }
    ],
    "grammars": [
      {
        "language": "proto3",
        "scopeName": "source.proto",
        "path": "./syntaxes/proto3.tmLanguage.json"
      },
      {
        "scopeName": "markdown.codeblock.proto",
        "path": "./syntaxes/proto3.codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
